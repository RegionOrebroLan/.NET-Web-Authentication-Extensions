{
	"Authentication": {
		"CallbackDecorators": {
			"Active-Directory-Callback-Decorator": {
				"AuthenticationSchemes": {
					"SITHS-Certificate": 100
				},
				"Type": "RegionOrebroLan.Web.Authentication.Decoration.Deprecated.ActiveDirectoryCallbackDecorator, RegionOrebroLan.Web.Authentication"
			},
			"Callback-Decorator": {
				"AuthenticationSchemes": {
					"*": 10
				},
				"Options": {
					"ClaimTypeExclusions": [ "http://schemas.xmlsoap.org/ws/2005/05/identity/claims/dns" ]
				},
				"Type": "RegionOrebroLan.Web.Authentication.Decoration.Deprecated.CallbackDecorator, RegionOrebroLan.Web.Authentication"
			},
			"Replacer": {
				"AuthenticationSchemes": {
					"Negotiate": 120,
					"SITHS-Certificate": 120
				},
				"Options": {
					"Replacements": {
						"nameid": "sub",
						"unique_name": "name"
					}
				},
				"Type": "RegionOrebroLan.Web.Authentication.Decoration.Deprecated.Replacer, RegionOrebroLan.Web.Authentication"
			},
			"Microsoft-To-Jwt-Replacer": {
				"AuthenticationSchemes": {
					"Negotiate": 110,
					"SITHS-Certificate": 110
				},
				"Type": "RegionOrebroLan.Web.Authentication.Decoration.Deprecated.MicrosoftToJwtReplacer, RegionOrebroLan.Web.Authentication"
			},
			"SITHS-Certificate-Subject-Extractor": {
				"AuthenticationSchemes": {
					"SITHS-Certificate": 200
				},
				"Options": {
					"CertificateSubjectClaimTypes": [ "http://schemas.xmlsoap.org/ws/2005/05/identity/claims/x500distinguishedname" ]
				},
				"Type": "RegionOrebroLan.Web.Authentication.Decoration.Deprecated.SithsCertificateSubjectExtractor, RegionOrebroLan.Web.Authentication"
			}
		},
		"SchemeRegistrations": {
			"Negotiate": {
				"DisplayName": "Windows",
				"Type": "RegionOrebroLan.Web.Authentication.DependencyInjection.Registration.NegotiateRegistrator, RegionOrebroLan.Web.Authentication"
			},
			"SITHS-Certificate": {
				"DisplayName": "SITHS-certificate",
				"Icon": "net-id",
				"Options": {
					"Validator": {
						"AllowedCertificateKinds": "Chained",
						"Chained": {
							"Matching": {
								"Criteria": [
									{
										"PropertyName": "Issuer",
										"ValuePattern": "CN=SITHS e-id*"
									}
								]
							}
						}
					}
				},
				"Type": "RegionOrebroLan.Web.Authentication.DependencyInjection.Registration.CertificateRegistrator, RegionOrebroLan.Web.Authentication"
			}
		}
	}
}